}

### ROW 4 ###

key META_LEFT {
    label:                              '\uef01'
    base:                               '\uef01'
    shift, capslock:                    '\uef01'
    alt:                                '\uef01'
    shift+alt, capslock+alt:	        '\uef01'
}

key AT {
    label:                              '@'
    base:                               '@'
    shift, capslock:                    '@'
    alt:                                '&'
    shift+alt, capslock+alt:            '&'
}

key COMMA {
    label:                      	','
    base:                               ','
    shift, capslock:                    ','
    alt:				';'
    shift+alt, capslock+alt:            ';'
}

key SPACE {
    label:                              ' '
    base:                               ' '
    shift, capslock:                    ' '
